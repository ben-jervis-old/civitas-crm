<h1>Users</h1>

<div class="col-md-12">
	<div class="input-group">
		<input type="text" name="user-search" value class="form-control" placeholder="Search">
	</div>
</div>

<% @users = User.all %>
<% @users.each do |user| %>
  <div class="List-group-row">
  	<div class="col-md-4">
		  <div class="list-group-item">
		  	<p><a href="<%=user_path(user)%>"><%= user.name %></a></p>
	  	</div>
  	</div>
  </div>
<% end %>
